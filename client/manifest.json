{
  "manifest_version": 2,
  "name": "Hackathon",
  "version": "0.1",
  "page_action": {
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "declarativeContent",
    "storage",
    "history",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}